global mode="release"

probe module( "fat" ).function( "fat_alloc_inode" ) {
	if ( mode == "debug" ) {
		println( "fat_alloc_inode" )
	} else {
		if ( mode == "s2e_debug" ) {
			s2e_message( "fat_alloc_inode" )
		} else {
			@ops_super_alloc_inode( $sb )
